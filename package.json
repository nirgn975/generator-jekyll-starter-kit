{
  "name": "generator-jekyll-starter-kit",
  "version": "2.3.2",
  "description": "Combine Jekyll and Google web-starter-kit to enjoy the best of both worlds",
  "homepage": "https://github.com/nirgn975/generator-jekyll-starter-kit",
  "author": {
    "name": "Nir Galon",
    "email": "nir@galon.io",
    "url": "http://www.nirgn.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Jekyll",
    "Google Web Starter Kit",
    "HTML5",
    "Jade",
    "CSS3",
    "SASS",
    "SCSS",
    "JavaScript",
    "ES6",
    "ES2015",
    "Gulp",
    "Travis-CI",
    "gh-pages",
    "Firebase",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "nyc": "^12.0.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "nirgn975/generator-jekyll-starter-kit",
  "license": "ISC"
}
