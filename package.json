{
  "name": "generator-jekyll-starter-kit",
  "version": "3.0.2",
  "description": "Combine Jekyll and Google web-starter-kit to enjoy the best of both worlds",
  "homepage": "https://github.com/nirgn975/generator-jekyll-starter-kit",
  "author": {
    "name": "Nir Galon",
    "email": "nir@galon.io",
    "url": "http://www.nirgn.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Jekyll",
    "Google Web Starter Kit",
    "HTML5",
    "Jade",
    "CSS3",
    "SASS",
    "SCSS",
    "JavaScript",
    "ES6",
    "ES2015",
    "Gulp",
    "Travis-CI",
    "gh-pages",
    "Firebase",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^2.0.0",
    "yeoman-assert": "^3.1.1",
    "eslint": "^6.5.1",
    "prettier": "^1.18.2",
    "husky": "^4.2.5",
    "lint-staged": "^9.4.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.29.1",
    "jest": "^24.9.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "nyc": "^14.1.1",
    "yeoman-generator": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "nirgn975/generator-jekyll-starter-kit",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@nirgn975"
  }
}
