{
  "name": "generator-jekyll-starter-kit",
  "version": "3.1.1",
  "description": "Combine Jekyll and Google web-starter-kit to enjoy the best of both worlds",
  "homepage": "https://github.com/nirgn975/generator-jekyll-starter-kit",
  "author": {
    "name": "Nir Galon",
    "email": "nir@galon.io",
    "url": "http://nir.galon.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Jekyll",
    "Google Web Starter Kit",
    "HTML5",
    "Jade",
    "CSS3",
    "SASS",
    "SCSS",
    "JavaScript",
    "ES6",
    "ES2015",
    "Gulp",
    "Travis-CI",
    "gh-pages",
    "Firebase",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-xo": "^0.34.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^4.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "nyc": "^15.1.0",
    "yeoman-generator": "^4.12.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "nirgn975/generator-jekyll-starter-kit",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
